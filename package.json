    "i18n",
    "node.js",
    "browser",
    "localization",
    "jstrings",
    "strings"
  ],
  "author": "Laurent STEFAN",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guitarneck/jstrings-localization/issues"
  },
  "homepage": "https://github.com/guitarneck/jstrings-localization#readme",
  "devDependencies": {
    "del": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-jsdoc3": "^1.0.1",
    "istanbul": "^0.4.5",
    "mocha": "^5.0.0",
    "opener": "^1.4.3"
  }
}
